<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ds.metrocabs.model.transaction.Timesheet" table="metrocabs_timesheet" schema="metrocabs">
<id name="timesheet_id">
<generator class="increment"></generator>
</id>
<property name="from_time"></property>
<property name="to_time"></property>
<property name="from_date"></property>
<property name="to_date"></property>
</class>
</hibernate-mapping>