<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ds.metrocabs.model.payment.InsuranceType" table="metrocabs_insu_typ" schema="metrocabs">
<id name="insurance_typ_id">
<generator class="increment"></generator>
</id>
<!-- insurance typ description with unique constraint -->
<property name="insurance_typ_desc" unique="true"></property>
</class>
</hibernate-mapping>