<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ds.metrocabs.model.payment.Coupon" table="metrocabs_coupon" schema="metrocabs">
<id name="coupon_id">
<generator class="increment"></generator>
</id>
<property name="max_amt"></property>
<property name="min_amt"></property>
<property name="valid_upto"></property>
<property name="discount_amt"></property>
</class>
</hibernate-mapping>