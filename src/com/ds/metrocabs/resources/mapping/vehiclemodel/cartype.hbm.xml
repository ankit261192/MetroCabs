<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ds.metrocabs.model.vehicle.CarType" table="metrocabs_car_typ" schema="metrocabs">
<id name="car_typ_id">
<generator class="increment"></generator>
</id>
<property name="car_typ_name" unique="true"></property>
<property name="car_typ_desc" unique="true"></property>
</class>
</hibernate-mapping>