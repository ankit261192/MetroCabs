<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ds.metrocabs.model.vehicle.FuelType" table="metrocabs_fuel_typ" schema="metrocabs">
<id name="fuel_typ_id">
<generator class="increment"></generator>
</id>
<property name="fuel_typ_name"></property>
<property name="fuel_typ_desc" unique="true"></property>
</class>
</hibernate-mapping>